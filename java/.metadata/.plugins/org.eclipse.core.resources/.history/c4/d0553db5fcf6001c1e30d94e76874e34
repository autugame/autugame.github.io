 */
public class StringDemo {

	public static void main(String[] args) {
		String s1 = "愤愤";
		String s2 = new String("愤愤");
		System.out.println(s1 + s2);
		String s3 = "纷纷";
		String a = "a";
		String a1 = a + 1;
		String a2 = "a1";
		System.out.println(a1 == a2);
	}

}
